{
    "name": "dan-lyn/laravel-uuid",
    "type": "library",
    "description": "Adds a uuid to a model using ramsey/uuid",
    "version": "0.0.1",
    "keywords": [
        "laravel",
        "uuid",
        "eloquent",
        "universally unique identifier",
        "ramsey uuid"
    ],
    "license": "MIT",
    "authors" : [
        {
          "name": "dan-lyn",
          "homepage": "https://github.com/dan-lyn/"
        }
    ],
    "support": {
        "issues": "https://github.com/dan-lyn/laravel-uuid/issues"
    },
    "require": {
        "php": "^7.1",
        "ramsey/uuid": "^3.7.3",
        "illuminate/database": "^5.6",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "DanLyn\\LaravelUuid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DanLyn\\LaravelUuid\\Tests\\": "tests/"
        }
    }
}
